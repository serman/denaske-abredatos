        }
        if (!$this->assertEqual($node->promote, 1, t('Validate first node promote'))) {
          $this->error('Actual node: ' . print_r($node, TRUE));
        }
        if (!$this->assertEqual($node->sticky, 1, t('Validate first node sticky'))) {
